#include <stdio.h>

int main() {
    int array[100];
    int size, i, sum = 0;
    
    printf("Enter the size: ");
    scanf("%d", &size);
    
    printf("Enter the elements:\n");
    for (i = 0; i < size; i++) {
        scanf("%d", &array[i]);
    }
    for (i = 0; i < size; i++) {
        sum += array[i];
    }
    
    printf("The sum of elements  is: %d\n", sum);
    
    return 0;
}
